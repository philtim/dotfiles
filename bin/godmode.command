#!/bin/bash
osascript <<EOD
set appname to "Self Service"

tell application appname
	activate
end tell

tell application "System Events"
	click UI element "Run" of group 1 of list 1 of list 1 of scroll area 1 of window 1 of application process "Self Service"
	delay 10
	click UI element "OK" of window "Admin rights granted" of application process "jamfHelper"
end tell

tell application appname
	quit
end tell
